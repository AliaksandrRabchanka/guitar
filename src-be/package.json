{
  "name": "guitar-be",
  "version": "1.0.0",
  "license": "MIT",
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts, json",
    "ignore": [
      "src/**/*.test.ts"
    ],
    "delay": 3,
    "execMap": {
      "ts": "ts-node"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npx nodemon src/index.ts",
    "start:prod": "tsc && node dist/src/index.js"
  },
  "dependencies": {
    "@types/jsonwebtoken": "^8.5.8",
    "activedirectory": "^0.7.2",
    "axios": "^0.26.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-node": "0.0.1-security",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mongodb": "^4.5.0",
    "nodemon": "^2.0.15",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.41",
    "uuid": "^9.0.0",
    "ws": "^8.11.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.13",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^17.0.23",
    "nodemon": "^2.0.15",
    "ts-loader": "^9.2.8",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.6.3"
  }
}
